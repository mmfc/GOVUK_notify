  "definitions": {
    "MessageResponse": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Required - notification ID",
          "title": "ID",
          "example": "740e5834-3a29-46b4-9a6f-16142fde533a"
        },
        "reference": {
          "type": "string",
          "description": "Optional reference for the messages",
          "title": "Reference",
          "example": "STRING"
        },
        "email_address": {
          "type": "string",
          "description": "The email the message was sent to",
          "title": "Email address",
          "example": "sender@something.com"
        },
        "phone_number": {
          "type": "string",
          "description": "The phone number the message was sent to",
          "title": "Phone number",
          "example": 447900900123
        },
        "line_1": {
          "type": "string",
          "description": "Required for letter",
          "title": "Address Line 1",
          "example": "ADDRESS LINE 1"
        },
        "line_2": {
          "type": "string",
          "description": "Required for letter",
          "title": "Address Line 2",
          "example": "ADDRESS LINE 2"
        },
        "line_3": {
          "type": "string",
          "description": "Required for letter",
          "title": "Address Line 3",
          "example": "ADDRESS LINE 3"
        },
        "line_4": {
          "type": "string",
          "title": "Address Line 4",
          "description": "Required for letter",
          "example": "ADDRESS LINE 4"
        },
        "line_5": {
          "type": "string",
          "title": "Address Line 5",
          "description": "Required for letter",
          "example": "ADDRESS LINE 5"
        },
        "line_6": {
          "type": "string",
